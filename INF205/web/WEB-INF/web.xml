<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>Controller.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerProducts</servlet-name>
        <servlet-class>Controller.ControllerProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CotrollerCustomers</servlet-name>
        <servlet-class>Controller.CotrollerCustomers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerCarBean</servlet-name>
        <servlet-class>Controller.ControllerCarBean</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerProducts</servlet-name>
        <url-pattern>/ControllerProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CotrollerCustomers</servlet-name>
        <url-pattern>/CotrollerCustomers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerCarBean</servlet-name>
        <url-pattern>/ControllerCarBean</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
